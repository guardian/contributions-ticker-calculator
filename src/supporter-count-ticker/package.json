{
  "name": "supporter-count-ticker",
  "isAwsLambda": true,
  "cloudformation": false,
  "projectName": "Contributions::supporter-count-ticker",
  "buildDir": "./target",
  "riffraffFile": "./riff-raff.yaml",
  "dependencies": {
    "aws-sdk": "^2.264.1",
    "typescript": "2.8.3"
  },
  "devDependencies": {
    "@types/node": "10.14.22",
    "node-riffraff-artefact": "^2.0.1"
  },
  "scripts": {
    "clean": "rm -rf target",
    "build": "tsc",
    "query-local": "node target/value-ticker/query-lambda/local.js",
    "calculate-local": "node target/supporter-count-ticker/calculate-lambda/local.js",
    "package": "ARTEFACT_PATH=$PWD/target VERBOSE=true riffraff-artefact"
  }
}
